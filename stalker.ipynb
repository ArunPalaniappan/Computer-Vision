   "source": [
    "import urllib.request\n",
    "import PIL.Image\n",
    "\n",
    "roll = input('Enter Roll Number: ')\n",
    "url = \"https://photos.iitm.ac.in/byroll.php?roll=\"+str(roll)\n",
    "urllib.request.urlretrieve(str(url), \"sample.png\")\n",
    "\n",
    "img = PIL.Image.open(\"sample.png\")\n",
    "img.show()"
   ]
